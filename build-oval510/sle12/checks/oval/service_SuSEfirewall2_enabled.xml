<def-group>

  <definition class="compliance" id="service_SuSEfirewall2_enabled"
  version="1">
    <metadata>
        <title>Enable the SuSEfirewall 2</title>
        
    <affected family="unix">
    <platform>SUSE Linux Enterprise 12</platform>
    </affected>
        <description>The SuSEfirewall2 service should be enabled if possible.</description>
    </metadata>
    <criteria comment="package SuSEfirewall2 installed and service SuSEfirewall2 is configured to start" operator="AND">
    <criterion comment="SuSEfirewall2 installed" test_ref="test_service_SuSEfirewall2_package_SuSEfirewall2_installed" />
    <criteria operator="OR" comment="service SuSEfirewall2 is configured to start">
      <criterion comment="SuSEfirewall2 runlevel 0" test_ref="test_runlevel0_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 1" test_ref="test_runlevel1_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 2" test_ref="test_runlevel2_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 3" test_ref="test_runlevel3_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 4" test_ref="test_runlevel4_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 5" test_ref="test_runlevel5_SuSEfirewall2_on" />
      <criterion comment="SuSEfirewall2 runlevel 6" test_ref="test_runlevel6_SuSEfirewall2_on" />
    </criteria>
    </criteria>
  </definition>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel0_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel0_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel1_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel1_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel2_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel2_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel3_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel3_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel4_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel4_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel5_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel5_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_test check="all" check_existence="any_exist"
  comment="Runlevel test" id="test_runlevel6_SuSEfirewall2_on"
  version="2">
    <unix:object object_ref="obj_runlevel6_SuSEfirewall2_on" />
    <unix:state state_ref="state_service_SuSEfirewall2_on" />
  </unix:runlevel_test>
  <unix:runlevel_object id="obj_runlevel0_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">0</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel1_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">1</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel2_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">2</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel3_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">3</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel4_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">4</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel5_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">5</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_object id="obj_runlevel6_SuSEfirewall2_on" version="1">
    <unix:service_name>SuSEfirewall2</unix:service_name>
    <unix:runlevel operation="equals">6</unix:runlevel>
  </unix:runlevel_object>
  <unix:runlevel_state comment="configured to start" id="state_service_SuSEfirewall2_on" version="1">
    <unix:start datatype="boolean">true</unix:start>
    <unix:kill datatype="boolean">false</unix:kill>
  </unix:runlevel_state>



  <linux:rpminfo_test check="all" check_existence="all_exist"
  id="test_service_SuSEfirewall2_package_SuSEfirewall2_installed" version="1"
  comment="package SuSEfirewall2 is installed">
    <linux:object object_ref="obj_test_service_SuSEfirewall2_package_SuSEfirewall2_installed" />
    
  </linux:rpminfo_test>
  <linux:rpminfo_object id="obj_test_service_SuSEfirewall2_package_SuSEfirewall2_installed" version="1">
    <linux:name>SuSEfirewall2</linux:name>
  </linux:rpminfo_object>
  

</def-group>